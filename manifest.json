{
	"manifest_version": 3,
	"name": "Discord Scraper",
	"version": "2.0", 
	"permissions": ["activeTab", "storage", "scripting"],
	"host_permissions": ["https://discord.com/*"],
	"background": {
	  	"service_worker": "background.js"
	},
	"content_scripts": [
		{
			"matches": ["https://discord.com/*"],
			"js": ["content.js"]
		}
	],
	"action": {
		"default_popup": "popup.html",
		"default_icon": {
			"128": "images/icon128.png"
		}
	}
}
  